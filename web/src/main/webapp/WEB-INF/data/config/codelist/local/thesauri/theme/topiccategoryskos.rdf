</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_geochemistry">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Studies that use the tools and principles of chemistry to solve geoscience problems; includes isotope geochemistry, biogeochemistry, organic geochemistry, regional, environmental and exploration geochemistry, rock geochemical exploration, soil geochemical exploration, stream sediment geochemical exploration, atmospheric geochemical exploration, biogeochemical exploration, water geochemical exploration, geo-gas survey, chemical analysis, geochemical mapping, geothermal manifestation, radiometric dating, etc.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.26 Geochemistry</skos:prefLabel>
	<skos:narrower rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_SG_surficialGeochemistry"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_SG_surficialGeochemistry">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of the chemical principles and processes active on the Earth surface</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.26.1 Surficial geochemistry</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_geochemistry"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

