	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of volcanoes, lava, magma and related geological, geophysical and geochemical phenomena (volcanism).(Wikipedia)</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.39 Volcanology</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
