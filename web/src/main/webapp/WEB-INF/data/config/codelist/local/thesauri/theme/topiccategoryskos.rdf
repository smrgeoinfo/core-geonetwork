<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_PO_molecularPaleontology">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Recovery and analysis of DNA, proteins, carbohydrates, or lipids, and their diagenetic products from ancient human, animal, and plant remains.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.2.8 Molecular Paleontology</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_PO_Paleontology"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_PO_Paleontology">
