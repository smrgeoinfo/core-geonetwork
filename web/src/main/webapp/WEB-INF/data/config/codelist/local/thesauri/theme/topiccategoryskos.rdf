	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of the geology of the celestial bodies such as the planets and their moons, asteroids, comets, and meteorites. Alternatively known as astrogeology or exogeology</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.37 Planetary geology</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
