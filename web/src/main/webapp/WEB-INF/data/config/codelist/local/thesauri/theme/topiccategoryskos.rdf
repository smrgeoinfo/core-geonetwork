<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_metamorphicFabrics">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_dynamicMetamorphism">
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology">
	<skos:scopeNote xml:lang="en">Study of the transformation of existing rock to new types of rock through solid-state mineralogical, chemical and/or structural changes.</skos:scopeNote>
	<skos:narrower rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_regionalMetamorphism"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_regionalMetamorphism">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of metamorphism that occurs over a wide extent, affecting a large rock volume, associated with large-scale tectonic processes, such as ocean-floor spreading, crustal thickening related to plate collision, deep basin subsidence, etc.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.6.6 Regional metamorphism</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology">
	<skos:prefLabel xml:lang="en">8.06 Metamorphic petrology</skos:prefLabel>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#geoscientificInformation"/>
	<skos:narrower rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_impactMetamorphism"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_impactMetamorphism">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of metamorphism caused by the passage of a shock wave due to the impact of a planetary body (projectile or impactor) on a planetary surface (target). It includes melting and vaporization of the target rock(s).</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.6.3 Impact metamorphism</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology">
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:narrower rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_burialMetamorphism"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_burialMetamorphism">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of metamorphism resulting from deep burial under a sedimentary or volcanic accumulation, typically not associated with deformation or magmatism.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.6.8 Burial metamorphism</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology">
	<skos:narrower rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_migmatite"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_migmatite">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of silicate metamorphic rock, pervasively heterogeneous on a meso- to megascopic scale, typically consisting of darker and lighter parts. The darker parts usually exhibit features of metamorphic rocks whereas the lighter parts are of igneous-looking appearance.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.6.9 Migmatite</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:narrower rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_oceanfloorMetamorphism"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_oceanfloorMetamorphism">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of metamorphism related to the steep geothermal gradients occurring near spreading centers in oceanic environments.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.6.7 Ocean-floor metamorphism</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology">
	<skos:narrower rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_contactMetamorphism"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_contactMetamorphism">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of metamorphism that affects the rocks adjacent to magma bodies emplaced in a variety of environments from volcanic to upper mantle depths, in both continental and oceanic settings.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.6.4 Contact metamorphism</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology">
	<skos:narrower rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_metasomatism"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_metasomatism">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of metamorphic processes through which the chemical composition of a rock body is altered, involving the introduction and/or removal of chemical components as a result of the interaction of the rock with aqueous or other fluids. During metasomatism the rock remains in a solid state.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.6.5 Metasomatism</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
	<skos:broader rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_MetamorphicPetrology">
	<skos:narrower rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_metamorphicHistory"/>
</rdf:Description>

<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMP_metamorphicHistory">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study of the temporal sequence of metamorphic facies, metamorphic series, metamorphic zones, and P-T-t paths in a rock or a geological unit. A metamorphic facies is a set of metamorphic mineral assemblages, repeatedly associated in time and space and showing a regular relationship between mineral composition and bulk chemical composition. A series of metamorphic facies developed under a particular range of P/T. A succession of metamorphic zones characterized by distinctive index minerals.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.6.10 Metamorphic history</skos:prefLabel>
</rdf:Description>

