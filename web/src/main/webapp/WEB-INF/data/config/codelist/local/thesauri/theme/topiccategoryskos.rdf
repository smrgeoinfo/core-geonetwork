<rdf:Description rdf:about="https://www.ddeworld.org/resource/codelist/topicCategory#GI_MMG_modelling">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study, documentation and development of conceptualizations that can be used to explain observed data, and implementation of these conceptualizations in software to compare predicted behavior to the actual observed data, or to predict the outcome of an event.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.14.3 Modelling</skos:prefLabel>
	<skos:inScheme rdf:resource="https://www.ddeworld.org/resource/codelist/topicCategory"/>
