	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">Study and documentation of techniques for graphical representation of geoscience information and data.</skos:scopeNote>
	<skos:prefLabel xml:lang="en">8.14.1 Data visualization‎</skos:prefLabel>
